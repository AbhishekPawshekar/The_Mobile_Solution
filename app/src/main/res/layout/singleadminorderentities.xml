<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="70dp"
    android:layout_marginTop="10dp"
    android:id="@+id/admindeleteorderui1"
    >


        <RelativeLayout
            android:id="@+id/adminorderui"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
<ImageView
    android:layout_width="50dp"
    android:layout_height="65dp"
    android:id="@+id/adminorderproductimage"/>
            <TextView
                android:id="@+id/adminorderfullname"
                android:layout_width="100dp"
                android:textColor="@color/purple"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:textStyle="bold"
                android:layout_marginLeft="60dp"
                android:fontFamily="serif"
                android:textSize="10dp"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/adminorderphone"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="105dp"
                android:textColor="@color/purple"

                android:layout_alignLeft="@+id/adminorderfullname"
                android:textStyle="bold"
                android:fontFamily="serif"
                android:textSize="10dp"
                android:gravity="center"
                />

            <TextView
                android:id="@+id/adminorderemail"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="105dp"
                android:textColor="@color/purple"
                android:layout_alignLeft="@+id/adminorderphone"
                android:textStyle="bold"
                android:fontFamily="serif"
                android:textSize="10dp"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/adminorderproductname"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="165dp"
                android:textStyle="bold"
                android:textColor="@color/purple"
                android:fontFamily="serif"
                android:gravity="center"
                android:layout_alignLeft="@+id/adminorderemail"
                android:textSize="10dp"

                />
            <TextView
                android:id="@+id/adminorderoriginalprice"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:textColor="@color/purple"
                android:layout_marginLeft="175dp"
                android:layout_alignLeft="@+id/adminorderproductname"
                android:textStyle="bold"
                android:fontFamily="serif"
                android:textSize="10dp"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/adminorderproductdiscount"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:textColor="@color/purple"
                android:layout_marginLeft="105dp"
                android:layout_alignLeft="@+id/adminorderoriginalprice"
                android:textStyle="bold"
                android:fontFamily="serif"
                android:textSize="10dp"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/adminorderdiscountprice"
                android:layout_width="100dp"
                android:textColor="@color/purple"
                android:layout_height="wrap_content"
                android:layout_marginLeft="105dp"
                android:layout_alignLeft="@+id/adminorderproductdiscount"
                android:textStyle="bold"
                android:fontFamily="serif"
                android:textSize="10dp"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/adminorderdeliverycharges"
                android:layout_width="130dp"
                android:textColor="@color/purple"
                android:layout_height="wrap_content"
                android:layout_marginLeft="105dp"
                android:layout_alignLeft="@+id/adminorderdiscountprice"
                android:textStyle="bold"
                android:fontFamily="serif"
                android:textSize="10dp"
                android:gravity="center"
                />
            <TextView
                android:id="@+id/adminorderoverallchargespaid"
                android:layout_width="150dp"
                android:textColor="@color/purple"
                android:layout_height="wrap_content"
                android:layout_marginLeft="140dp"
                android:layout_alignLeft="@+id/adminorderdeliverycharges"
                android:textStyle="bold"
                android:fontFamily="serif"
                android:textSize="10dp"
                android:gravity="center"
                />
            <TextView
            android:id="@+id/adminorderwayofdelivery"
            android:layout_width="150dp"
            android:textColor="@color/purple"
            android:layout_height="wrap_content"
            android:layout_marginLeft="160dp"
            android:layout_alignLeft="@+id/adminorderoverallchargespaid"
            android:textStyle="bold"
            android:fontFamily="serif"
            android:textSize="12dp"
            android:gravity="center_horizontal"
            />
            <TextView
                android:id="@+id/adminorderaddress"
                android:layout_width="200dp"
                android:textColor="@color/purple"
                android:layout_height="wrap_content"
                android:layout_marginLeft="155dp"
                android:layout_alignLeft="@+id/adminorderwayofdelivery"
                android:textStyle="bold"
                android:fontFamily="serif"
                android:textSize="12dp"
                android:gravity="center_horizontal"
                />

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:id="@+id/admindeleteorderbbutton"
                android:layout_alignLeft="@+id/adminorderaddress"
                android:layout_marginLeft="205dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/delete_producticon"/>

        </RelativeLayout>

</androidx.cardview.widget.CardView>