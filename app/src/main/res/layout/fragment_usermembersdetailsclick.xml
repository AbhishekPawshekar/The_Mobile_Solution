<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".usermembersdetailsclick"
   >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <!-- TODO: Update blank fragment layout -->
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="39dp"
            android:layout_alignRight="@+id/centerfinder">

            <TextView
                android:id="@+id/request_to_alluser"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:fontFamily="serif"
                android:gravity="center"
                android:text="All User"
                android:textColor="@color/purple" />
        </RelativeLayout>

        <TextView
            android:id="@+id/centerfinder"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="39dp"
            android:layout_alignLeft="@+id/centerfinder">

            <TextView
                android:id="@+id/request_to_allmembers"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:fontFamily="serif"
                android:textColor="@color/purple"
                android:gravity="center"
                android:text="All Member's" />

        </RelativeLayout>
            <FrameLayout
                android:id="@+id/usermemberframelayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="40dp" />

    </RelativeLayout>
</FrameLayout>